alias art='valet php artisan'
alias t='valet composer test'
alias td='valet php artisan dusk'
alias tdf='valet php artisan dusk:fails'
alias ta='t; td'
alias ads='valet php artisan dump-server'
alias sail='[ -f sail ] && bash sail || bash vendor/bin/sail'
alias sart='sail artisan'

alias mfs='valet php artisan migrate:fresh --seed'
alias smf='sail artisan migrate:fresh --seed'
